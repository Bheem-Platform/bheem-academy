/* Base Setup */
.ultimate-footer {
    position: relative;
    margin-top: 120px;
    background: #0a0a0a;
    overflow: hidden;
    font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
}

/* Aurora Background */
.aurora-bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    z-index: 1;
}

.aurora-layer {
    position: absolute;
    width: 200%;
    height: 200%;
    opacity: 0.4;
}

.aurora-1 {
    background: radial-gradient(ellipse at center, #8b5cf6 0%, transparent 40%);
    animation: aurora1 20s ease-in-out infinite;
}

.aurora-2 {
    background: radial-gradient(ellipse at center, #ec4899 0%, transparent 40%);
    animation: aurora2 25s ease-in-out infinite;
}

.aurora-3 {
    background: radial-gradient(ellipse at center, #3b82f6 0%, transparent 40%);
    animation: aurora3 30s ease-in-out infinite;
}

@keyframes aurora1 {
    0%, 100% { transform: translate(-50%, -50%) rotate(0deg) scale(1); }
    50% { transform: translate(-30%, -70%) rotate(180deg) scale(1.5); }
}

@keyframes aurora2 {
    0%, 100% { transform: translate(-50%, -50%) rotate(0deg) scale(1.5); }
    50% { transform: translate(-70%, -30%) rotate(-180deg) scale(1); }
}

@keyframes aurora3 {
    0%, 100% { transform: translate(-50%, -50%) rotate(0deg) scale(1.2); }
    50% { transform: translate(-40%, -60%) rotate(90deg) scale(1.8); }
}

/* Gradient Mesh Canvas */
.gradient-mesh {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
    opacity: 0.5;
}

/* Floating 3D Elements */
.floating-elements {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 3;
    pointer-events: none;
}

.float-element {
    position: absolute;
    animation: float-3d 20s ease-in-out infinite;
}

.float-element:nth-child(1) {
    top: 20%;
    left: 10%;
}

.float-element:nth-child(2) {
    top: 60%;
    right: 15%;
}

@keyframes float-3d {
    0%, 100% { transform: translateY(0) translateZ(0) rotateX(0) rotateY(0); }
    25% { transform: translateY(-20px) translateZ(50px) rotateX(15deg) rotateY(45deg); }
    50% { transform: translateY(20px) translateZ(-50px) rotateX(-15deg) rotateY(90deg); }
    75% { transform: translateY(-10px) translateZ(30px) rotateX(10deg) rotateY(135deg); }
}

/* 3D Cube */
.cube {
    width: 60px;
    height: 60px;
    transform-style: preserve-3d;
    animation: rotate-cube 10s linear infinite;
}

.cube-face {
    position: absolute;
    width: 60px;
    height: 60px;
    background: linear-gradient(135deg, rgba(139, 92, 246, 0.2), rgba(236, 72, 153, 0.2));
    border: 1px solid rgba(139, 92, 246, 0.3);
    backdrop-filter: blur(10px);
}

.cube-front { transform: translateZ(30px); }
.cube-back { transform: translateZ(-30px) rotateY(180deg); }
.cube-left { transform: translateX(-30px) rotateY(-90deg); }
.cube-right { transform: translateX(30px) rotateY(90deg); }
.cube-top { transform: translateY(-30px) rotateX(90deg); }
.cube-bottom { transform: translateY(30px) rotateX(-90deg); }

@keyframes rotate-cube {
    from { transform: rotateX(0) rotateY(0); }
    to { transform: rotateX(360deg) rotateY(360deg); }
}

/* 3D Sphere */
.sphere {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    background: radial-gradient(circle at 30% 30%, rgba(139, 92, 246, 0.6), rgba(236, 72, 153, 0.2));
    box-shadow: 
        inset -10px -10px 20px rgba(0, 0, 0, 0.5),
        0 0 40px rgba(139, 92, 246, 0.5);
    animation: pulse-sphere 3s ease-in-out infinite;
}

@keyframes pulse-sphere {
    0%, 100% { transform: scale(1); box-shadow: 0 0 40px rgba(139, 92, 246, 0.5); }
    50% { transform: scale(1.1); box-shadow: 0 0 60px rgba(236, 72, 153, 0.8); }
}

/* Activity Ticker */
.activity-ticker {
    position: relative;
    z-index: 10;
    background: rgba(255, 255, 255, 0.03);
    backdrop-filter: blur(20px);
    border-bottom: 1px solid rgba(255, 255, 255, 0.1);
    padding: 15px 0;
    overflow: hidden;
}

.ticker-content {
    display: flex;
    animation: ticker-scroll 30s linear infinite;
    gap: 50px;
}

.ticker-item {
    display: flex;
    align-items: center;
    gap: 10px;
    white-space: nowrap;
    color: rgba(255, 255, 255, 0.8);
    font-size: 14px;
}

.ticker-item i {
    color: #8b5cf6;
}

@keyframes ticker-scroll {
    from { transform: translateX(0); }
    to { transform: translateX(-50%); }
}

/* Footer Content Wrapper */
.footer-content-wrapper {
    position: relative;
    z-index: 10;
    padding: 80px 0;
}

/* Hero Section */
.footer-hero {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: center;
    margin-bottom: 80px;
    padding: 0 40px;
}

.hero-title {
    font-size: clamp(3rem, 6vw, 5rem);
    font-weight: 900;
    line-height: 1.1;
    margin-bottom: 20px;
}

.title-word {
    display: inline-block;
    background: linear-gradient(135deg, #8b5cf6, #ec4899, #3b82f6);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    animation: gradient-shift 3s ease-in-out infinite;
}

.title-word:nth-child(2) { animation-delay: 0.1s; }
.title-word:nth-child(3) { animation-delay: 0.2s; }

@keyframes gradient-shift {
    0%, 100% { filter: hue-rotate(0deg); }
    50% { filter: hue-rotate(30deg); }
}

.hero-subtitle {
    color: rgba(255, 255, 255, 0.7);
    font-size: 1.5rem;
    margin-bottom: 30px;
}

.hero-cta {
    display: flex;
    gap: 20px;
}

.cta-button {
    position: relative;
    padding: 15px 40px;
    font-size: 16px;
    font-weight: 600;
    text-decoration: none;
    border-radius: 50px;
    overflow: hidden;
    transition: all 0.3s ease;
    text-align: center;
}

.cta-button.primary {
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    color: white;
}

.cta-button.secondary {
    background: transparent;
    color: white;
    border: 2px solid rgba(255, 255, 255, 0.3);
}

.button-bg {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    background: rgba(255, 255, 255, 0.2);
    border-radius: 50%;
    transform: translate(-50%, -50%);
    transition: all 0.5s ease;
}

.cta-button:hover .button-bg {
    width: 300px;
    height: 300px;
}

/* Orbit System */
.orbit-system {
    position: relative;
    width: 400px;
    height: 400px;
}

.orbit {
    position: absolute;
    border: 1px solid rgba(139, 92, 246, 0.2);
    border-radius: 50%;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.orbit-1 {
    width: 200px;
    height: 200px;
    animation: orbit-rotate 20s linear infinite;
}

.orbit-2 {
    width: 300px;
    height: 300px;
    animation: orbit-rotate 30s linear infinite reverse;
}

.orbit-3 {
    width: 400px;
    height: 400px;
    animation: orbit-rotate 40s linear infinite;
}

.planet {
    position: absolute;
    width: 50px;
    height: 50px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 20px;
    box-shadow: 0 0 30px rgba(139, 92, 246, 0.6);
}

.orbit-1 .planet { top: -25px; left: calc(50% - 25px); }
.orbit-2 .planet { top: -25px; left: calc(50% - 25px); }
.orbit-3 .planet { top: -25px; left: calc(50% - 25px); }

@keyframes orbit-rotate {
    from { transform: translate(-50%, -50%) rotate(0deg); }
    to { transform: translate(-50%, -50%) rotate(360deg); }
}

/* Cards Section */
.footer-cards-section {
    padding: 0 40px;
    margin-bottom: 80px;
}

.cards-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 30px;
}

.footer-card {
    position: relative;
    background: rgba(255, 255, 255, 0.03);
    backdrop-filter: blur(20px);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 24px;
    padding: 40px;
    transition: all 0.3s ease;
    cursor: pointer;
    transform-style: preserve-3d;
}

.card-glow {
    position: absolute;
    top: -2px;
    left: -2px;
    right: -2px;
    bottom: -2px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899, #3b82f6);
    border-radius: 24px;
    opacity: 0;
    transition: opacity 0.3s ease;
    z-index: -1;
    filter: blur(10px);
}

.footer-card:hover .card-glow {
    opacity: 0.5;
}

/* Brand Card */
.text-logo-3d {
    font-size: 2.5rem;
    font-weight: 900;
    line-height: 1;
    margin-bottom: 20px;
}

.logo-main {
    display: block;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
}

.logo-sub {
    display: block;
    color: rgba(255, 255, 255, 0.9);
    font-size: 1.8rem;
}

.brand-description {
    color: rgba(255, 255, 255, 0.7);
    line-height: 1.7;
    margin-bottom: 30px;
}

/* Social Carousel */
.social-carousel {
    overflow: hidden;
    margin-top: 30px;
}

.carousel-track {
    display: flex;
    gap: 15px;
    animation: carousel-slide 20s linear infinite;
}

@keyframes carousel-slide {
    from { transform: translateX(0); }
    to { transform: translateX(-50%); }
}

.social-icon-3d {
    width: 50px;
    height: 50px;
    background: rgba(255, 255, 255, 0.05);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 16px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 20px;
    transition: all 0.3s ease;
}

.social-item:hover .social-icon-3d {
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    transform: translateY(-5px) rotateY(360deg);
    box-shadow: 0 10px 30px rgba(139, 92, 246, 0.5);
}

/* Navigation Card */
.card-title {
    display: flex;
    align-items: center;
    gap: 10px;
    color: white;
    font-size: 1.5rem;
    font-weight: 700;
    margin-bottom: 25px;
}

.title-icon {
    width: 40px;
    height: 40px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 18px;
}

.nav-links {
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.nav-link {
    display: flex;
    align-items: center;
    padding: 15px;
    background: rgba(255, 255, 255, 0.03);
    border: 1px solid rgba(255, 255, 255, 0.05);
    border-radius: 12px;
    color: rgba(255, 255, 255, 0.8);
    text-decoration: none;
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
}

.nav-link::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(139, 92, 246, 0.2), transparent);
    transition: left 0.5s ease;
}

.nav-link:hover::before {
    left: 100%;
}

.nav-link:hover {
    background: rgba(255, 255, 255, 0.05);
    border-color: rgba(139, 92, 246, 0.3);
    transform: translateX(5px);
}

.link-icon {
    width: 30px;
    height: 30px;
    background: rgba(139, 92, 246, 0.1);
    border-radius: 8px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #8b5cf6;
    margin-right: 15px;
}

.link-text {
    flex: 1;
}

.link-arrow {
    color: rgba(255, 255, 255, 0.3);
    transition: all 0.3s ease;
}

.nav-link:hover .link-arrow {
    color: #8b5cf6;
    transform: translateX(5px);
}

/* Features Card */
.features-grid {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 20px;
}

.feature-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 10px;
    padding: 20px;
    background: rgba(255, 255, 255, 0.03);
    border-radius: 12px;
    transition: all 0.3s ease;
}

.feature-item:hover {
    background: rgba(139, 92, 246, 0.1);
    transform: translateY(-5px);
}

.feature-icon {
    width: 50px;
    height: 50px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 20px;
}

.feature-item span {
    color: rgba(255, 255, 255, 0.8);
    font-size: 14px;
    text-align: center;
}

/* Newsletter Card */
.newsletter-header {
    margin-bottom: 20px;
}

.newsletter-badge {
    display: inline-flex;
    align-items: center;
    gap: 5px;
    background: rgba(236, 72, 153, 0.1);
    border: 1px solid rgba(236, 72, 153, 0.3);
    color: #ec4899;
    padding: 5px 15px;
    border-radius: 20px;
    font-size: 12px;
    margin-bottom: 10px;
}

.offer-box {
    display: flex;
    align-items: center;
    gap: 15px;
    background: rgba(139, 92, 246, 0.1);
    border: 1px solid rgba(139, 92, 246, 0.2);
    border-radius: 12px;
    padding: 20px;
    margin-bottom: 25px;
}

.offer-icon {
    width: 40px;
    height: 40px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border-radius: 10px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 20px;
    animation: bounce 2s ease-in-out infinite;
}

@keyframes bounce {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-10px); }
}

.offer-text {
    color: rgba(255, 255, 255, 0.8);
    font-size: 14px;
}

.offer-text strong {
    color: #ec4899;
    font-size: 18px;
}

/* Ultra Newsletter Form */
.ultra-newsletter-form {
    margin-bottom: 20px;
}

.form-field {
    position: relative;
    margin-bottom: 20px;
}

.form-input {
    width: 100%;
    padding: 15px 20px;
    background: rgba(255, 255, 255, 0.05);
    border: 2px solid rgba(255, 255, 255, 0.1);
    border-radius: 12px;
    color: white;
    font-size: 16px;
    transition: all 0.3s ease;
}

.form-label {
    position: absolute;
    left: 20px;
    top: 50%;
    transform: translateY(-50%);
    color: rgba(255, 255, 255, 0.5);
    transition: all 0.3s ease;
    pointer-events: none;
}

.form-input:focus,
.form-input:not(:placeholder-shown) {
    background: rgba(255, 255, 255, 0.08);
    border-color: #8b5cf6;
}

.form-input:focus + .form-label,
.form-input:not(:placeholder-shown) + .form-label {
    top: -10px;
    left: 10px;
    font-size: 12px;
    background: #0a0a0a;
    padding: 0 10px;
    color: #8b5cf6;
}

.input-glow {
    position: absolute;
    top: -2px;
    left: -2px;
    right: -2px;
    bottom: -2px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border-radius: 12px;
    opacity: 0;
    filter: blur(10px);
    transition: opacity 0.3s ease;
    z-index: -1;
}

.form-input:focus ~ .input-glow {
    opacity: 0.5;
}

.submit-button {
    width: 100%;
    padding: 18px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border: none;
    border-radius: 12px;
    color: white;
    font-size: 16px;
    font-weight: 600;
    cursor: pointer;
    position: relative;
    overflow: hidden;
    transition: all 0.3s ease;
}

.button-content {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 10px;
    position: relative;
    z-index: 1;
}

.button-shimmer {
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.3), transparent);
    animation: shimmer 3s ease-in-out infinite;
}

@keyframes shimmer {
    0% { left: -100%; }
    100% { left: 100%; }
}

.submit-button:hover {
    transform: translateY(-2px);
    box-shadow: 0 10px 30px rgba(139, 92, 246, 0.5);
}

.submit-button.loading .button-text {
    opacity: 0;
}

.submit-button.loading .button-icon::after {
    content: '';
    position: absolute;
    width: 20px;
    height: 20px;
    border: 2px solid rgba(255, 255, 255, 0.3);
    border-top-color: white;
    border-radius: 50%;
    animation: spin 0.8s linear infinite;
}

.submit-button.success {
    background: #10b981;
}

.submit-button.success .button-text::after {
    content: 'Success!';
}

/* Trust Badges */
.trust-badges {
    display: flex;
    justify-content: space-around;
    gap: 10px;
}

.badge-item {
    display: flex;
    align-items: center;
    gap: 5px;
    color: rgba(255, 255, 255, 0.6);
    font-size: 12px;
}

.badge-item i {
    color: #8b5cf6;
}

/* Testimonials Section */
.testimonials-section {
    padding: 0 40px;
    margin-bottom: 80px;
}

.section-title {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 40px;
}

.title-line {
    flex: 1;
    height: 1px;
    background: linear-gradient(90deg, transparent, rgba(139, 92, 246, 0.5), transparent);
}

.title-text {
    color: white;
    font-size: 2rem;
    font-weight: 700;
    padding: 0 30px;
}

.testimonials-carousel {
    position: relative;
    max-width: 800px;
    margin: 0 auto;
}

.testimonial-item {
    background: rgba(255, 255, 255, 0.03);
    backdrop-filter: blur(20px);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 24px;
    padding: 40px;
    text-align: center;
}

.quote-icon {
    font-size: 40px;
    color: #8b5cf6;
    margin-bottom: 20px;
}

.testimonial-text {
    color: rgba(255, 255, 255, 0.8);
    font-size: 18px;
    line-height: 1.6;
    margin-bottom: 30px;
}

.testimonial-author {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 15px;
}

.testimonial-author img {
    width: 60px;
    height: 60px;
    border-radius: 50%;
}

.author-info h4 {
    color: white;
    font-size: 16px;
    margin: 0 0 5px;
}

.author-info p {
    color: rgba(255, 255, 255, 0.6);
    font-size: 14px;
    margin: 0;
}

/* Contact Section */
.contact-section {
    padding: 0 40px;
    margin-bottom: 60px;
}

.contact-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 30px;
}

.contact-method {
    display: flex;
    align-items: center;
    gap: 20px;
    padding: 30px;
    background: rgba(255, 255, 255, 0.03);
    backdrop-filter: blur(20px);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 20px;
    transition: all 0.3s ease;
}

.contact-method:hover {
    background: rgba(255, 255, 255, 0.05);
    transform: translateY(-5px);
    border-color: rgba(139, 92, 246, 0.3);
}

.method-icon {
    width: 60px;
    height: 60px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border-radius: 16px;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 24px;
}

.method-info h4 {
    color: white;
    font-size: 18px;
    margin: 0 0 5px;
}

.method-info p {
    color: rgba(255, 255, 255, 0.7);
    font-size: 14px;
    margin: 0;
}

/* Footer Bottom */
.footer-bottom-section {
    background: rgba(0, 0, 0, 0.5);
    backdrop-filter: blur(20px);
    padding: 30px 0;
    position: relative;
    z-index: 10;
}

.bottom-content {
    display: grid;
    grid-template-columns: 1fr auto 1fr;
    align-items: center;
    gap: 30px;
    padding: 0 40px;
}

.copyright {
    color: rgba(255, 255, 255, 0.6);
    font-size: 14px;
}

.copyright .pulse {
    color: #ec4899;
    animation: pulse-heart 1.5s ease-in-out infinite;
}

@keyframes pulse-heart {
    0%, 100% { transform: scale(1); }
    50% { transform: scale(1.2); }
}

.bottom-nav {
    display: flex;
    gap: 20px;
}

.bottom-link {
    color: rgba(255, 255, 255, 0.6);
    text-decoration: none;
    font-size: 14px;
    transition: all 0.3s ease;
    position: relative;
}

.bottom-link::after {
    content: '';
    position: absolute;
    bottom: -2px;
    left: 0;
    width: 0;
    height: 2px;
    background: linear-gradient(90deg, #8b5cf6, #ec4899);
    transition: width 0.3s ease;
}

.bottom-link:hover {
    color: white;
}

.bottom-link:hover::after {
    width: 100%;
}

.payment-partners {
    display: flex;
    align-items: center;
    gap: 15px;
    justify-content: flex-end;
}

.partners-label {
    color: rgba(255, 255, 255, 0.6);
    font-size: 14px;
}

.partner-logos {
    display: flex;
    gap: 15px;
}

.partner-logos i {
    font-size: 24px;
    color: rgba(255, 255, 255, 0.4);
    transition: all 0.3s ease;
}

.partner-logos i:hover {
    color: white;
    transform: scale(1.2);
}

/* Cursor Effect */
.cursor-effect {
    position: fixed;
    width: 20px;
    height: 20px;
    background: radial-gradient(circle, rgba(139, 92, 246, 0.5), transparent);
    border-radius: 50%;
    pointer-events: none;
    transform: translate(-50%, -50%);
    z-index: 9999;
    mix-blend-mode: screen;
}

/* Advanced Back to Top */
.advanced-backtop {
    position: fixed;
    bottom: 40px;
    right: 40px;
    width: 60px;
    height: 60px;
    cursor: pointer;
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
    z-index: 999;
}

.advanced-backtop.visible {
    opacity: 1;
    visibility: visible;
}

.backtop-ring {
    position: absolute;
    width: 100%;
    height: 100%;
    border: 2px solid rgba(139, 92, 246, 0.3);
    border-radius: 50%;
    animation: ring-pulse 2s ease-in-out infinite;
}

@keyframes ring-pulse {
    0%, 100% { transform: scale(1); opacity: 1; }
    50% { transform: scale(1.2); opacity: 0.5; }
}

.backtop-core {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 40px;
    height: 40px;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 18px;
    box-shadow: 0 5px 20px rgba(139, 92, 246, 0.5);
    transition: all 0.3s ease;
}

.advanced-backtop:hover .backtop-core {
    transform: translate(-50%, -50%) scale(1.1);
    box-shadow: 0 8px 30px rgba(139, 92, 246, 0.7);
}

.backtop-svg {
    position: absolute;
    top: 0;
    left: 0;
