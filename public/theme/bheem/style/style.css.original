/* AI Elements Container */
.ai-elements-container {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    z-index: 1;
}

/* Floating AI Elements */
.ai-element {
    position: absolute;
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 5px;
    cursor: pointer;
    pointer-events: all;
    transition: transform 0.3s ease;
}

.ai-element:hover {
    transform: scale(1.2) !important;
}

.element-icon {
    width: 50px;
    height: 50px;
    background: rgba(139, 92, 246, 0.1);
    backdrop-filter: blur(10px);
    border: 1px solid rgba(139, 92, 246, 0.3);
    border-radius: 12px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 20px;
    color: white;
    font-weight: bold;
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
}

.element-icon::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    background: radial-gradient(circle, rgba(139, 92, 246, 0.6), transparent);
    transition: all 0.3s ease;
    transform: translate(-50%, -50%);
}

.ai-element:hover .element-icon::before {
    width: 100px;
    height: 100px;
}

.ai-element[data-tech="neural-network"] .element-icon {
    background: rgba(59, 130, 246, 0.1);
    border-color: rgba(59, 130, 246, 0.3);
}

.ai-element[data-tech="llama"] .element-icon {
    background: rgba(236, 72, 153, 0.1);
    border-color: rgba(236, 72, 153, 0.3);
}

.ai-element[data-tech="nocode"] .element-icon {
    background: rgba(34, 197, 94, 0.1);
    border-color: rgba(34, 197, 94, 0.3);
}

.ai-element[data-tech="fine-tuning"] .element-icon {
    background: rgba(251, 146, 60, 0.1);
