    color: rgba(255, 255, 255, 0.4);
    transition: all 0.3s ease;
}

.payment-badges i:hover {
    color: white;
    transform: scale(1.15);
}

/* Chat Widget - Enhanced */
.chat-widget {
    position: fixed;
    bottom: 36px;
    right: 36px;
    z-index: 1000;
}

.chat-trigger {
    width: 64px;
    height: 64px;
    border-radius: 50%;
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    border: none;
    color: white;
    font-size: 28px;
    cursor: pointer;
    box-shadow: 0 6px 24px rgba(139, 92, 246, 0.4);
    transition: all 0.3s ease;
    position: relative;
}

.chat-trigger:hover {
    transform: scale(1.1);
    box-shadow: 0 10px 36px rgba(139, 92, 246, 0.5);
}

.chat-badge {
    position: absolute;
    top: -6px;
    right: -6px;
    width: 24px;
    height: 24px;
    background: #ff6b6b;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 13px;
    font-weight: 700;
    border: 2px solid #0a0a0a;
}

/* Modern Back to Top - Enhanced */
.modern-backtop-opt {
    position: fixed;
    bottom: 36px;
    left: 36px;
    width: 52px;
    height: 52px;
    border-radius: 50%;
    background: rgba(30, 27, 75, 0.9);
    border: 1px solid rgba(139, 92, 246, 0.2);
    color: white;
    cursor: pointer;
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
    z-index: 999;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 20px;
}

.modern-backtop-opt.visible {
    opacity: 1;
    visibility: visible;
}

.backtop-progress {
    position: absolute;
    top: -3px;
    left: -3px;
    width: calc(100% + 6px);
    height: calc(100% + 6px);
    border-radius: 50%;
    background: conic-gradient(#8b5cf6 0%, transparent 0%);
    z-index: -1;
}

.modern-backtop-opt:hover {
    background: linear-gradient(135deg, #8b5cf6, #ec4899);
    transform: scale(1.1);
    box-shadow: 0 8px 24px rgba(139, 92, 246, 0.5);
}

/* Responsive Design */
@media (max-width: 1200px) {
    .footer-grid-optimized {
        grid-template-columns: repeat(2, 1fr);
        gap: 40px;
    }
    
    .stats-grid-compact {
        grid-template-columns: repeat(2, 1fr);
    }
}

@media (max-width: 768px) {
    .footer-grid-optimized {
        grid-template-columns: 1fr;
    }
    
    .stats-grid-compact {
        grid-template-columns: 1fr;
        gap: 20px;
    }
    
    .stat-item-compact {
        justify-content: flex-start;
    }
    
    .bottom-content-opt {
        flex-direction: column;
        text-align: center;
    }
    
    .legal-links-opt {
        flex-wrap: wrap;
        justify-content: center;
        gap: 16px;
    }
    
    .featured-courses-bar {
        flex-direction: column;
        align-items: flex-start;
        gap: 16px;
    }
    
    .courses-slider {
        width: 100%;
    }
    
    .text-logo-opt {
        font-size: 32px;
    }
    
    .column-title {
        font-size: 18px;
    }
    
    .footer-link-opt {
        font-size: 15px;
    }
}/* Fix for menu white shadow/emboss effect */

/* Remove backdrop filter and white overlays from navigation menus */
.navbar,
.navbar .dropdown-menu,
.navbar-nav,
nav,
.nav-menu,
.navigation,
.menu-wrapper,
.dropdown,
.dropdown-toggle,
.dropdown-item {
    backdrop-filter: none !important;
    -webkit-backdrop-filter: none !important;
    filter: none !important;
    text-shadow: none !important;
}

/* Ensure clean background for dropdown menus */
.navbar .dropdown-menu,
.dropdown-menu {
    background-color: #ffffff !important;
    backdrop-filter: none !important;
    -webkit-backdrop-filter: none !important;
}

/* Remove any white overlay effects */
.navbar::before,
.navbar::after,
.navbar .dropdown-menu::before,
.navbar .dropdown-menu::after,
.nav-menu::before,
.nav-menu::after {
    display: none !important;
}

/* Ensure text is clearly readable */
.navbar .nav-link,
.navbar .dropdown-item,
.dropdown-menu .dropdown-item,
.nav-item a,
.menu-item a {
    text-shadow: none !important;
    color: #00261B !important;
    background: transparent !important;
}

/* Fix hover states */
.navbar .nav-link:hover,
.navbar .dropdown-item:hover {
    color: #126849 !important;
    background-color: rgba(18, 104, 73, 0.05) !important;
    text-shadow: none !important;
}

/* Remove any blur or filter from the main content area that might affect menus */
.page-wrapper,
.main-wrapper,
.content-wrapper,
#page,
#region-main {
    backdrop-filter: none !important;
    -webkit-backdrop-filter: none !important;
    filter: none !important;
}

/* Specific fix for any white shadow emboss effect */
* {
    text-shadow: none !important;
}

/* Ensure dropdown menus have proper contrast */
.dropdown-menu {
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1) !important;
    border: 1px solid #e0e0e0 !important;
}

/* Remove any gradient overlays that might cause the emboss effect */
.navbar,
.dropdown-menu,
nav {
    background-image: none !important;
