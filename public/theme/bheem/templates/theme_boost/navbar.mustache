<!-- Start Navbar Area --> 
<div class="navbar-area navbar navbar-expand-lg bg-white {{output.edoo_is_siteadmin}}" id="navbar">
    <div class="container-fluid">
        {{#logo_visibility}}
            <a class="navbar-brand me-90 p-0" href="{{{ config.wwwroot }}}/">
                {{#main_logo}}
                    <img class="main-logo" src="{{main_logo}}" alt="{{sitename}}"  {{#logo_styles}} style="{{{logo_styles}}}" {{/logo_styles}}>
                {{/main_logo}}

                {{^ main_logo }}
                    <h2  {{#logo_styles}} style="{{{logo_styles}}}" {{/logo_styles}}>{{sitename}}</h2>
                {{/main_logo}}
            </a>
        {{/logo_visibility}}

        <a class="navbar-toggler toggle-btn d-lg-none" data-toggle="offcanvas" href="#navbarOffcanvas" role="button" aria-controls="navbarOffcanvas">
            <span class="burger-menu">
                <span class="top-bar"></span>
                <span class="middle-bar"></span>
                <span class="bottom-bar"></span>
            </span>
        </a>

        <div class="others-options">
            <ul class="d-flex align-items-center ps-0 mb-0 list-unstyled">
                {{#category_title}}
                    <li class="d-none d-xxl-block">
                        <div class="dropdown">
                            <button class="btn dropdown-toggle" type="button" data-toggle="dropdown" aria-expanded="false">
                                {{{category_title}}}
                            </button>
                            <ul class="dropdown-menu">
                                {{{category_content}}}
                            </ul>
                        </div>
                    </li>
                {{/category_title}}

                <li class="me-0 ms-lg-0">
                    {{#header_search_bar}}
                        {{{edoo_globalsearch_navbar}}}
                    {{/header_search_bar}}
                </li>
                <li class="d-none d-sm-block d-lg-none">
                    <a href="schedule.html" class="btn btn-primary">
                        Get In Touch
                        <i class="flaticon-right-arrow"></i>
                    </a>
                </li>
            </ul>
        </div>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ms-auto">
                <!-- custom_menu -->
                {{{ output.custom_menu }}}
            </ul>
        </div>

        {{output.if_edoo_guest_user}}
        {{^ isloggedin}}
            <div class="others-options ms-50 d-none d-lg-block">
                {{#header_left_btn_text}}
                    <div class="option-item">
                        {{#header_left_btn_url}} <a href="{{header_left_btn_url}}" class="btn btn-primary"> {{/header_left_btn_url}}
                        {{^ header_left_btn_url }} <a href="{{ login_url }}" class="btn btn-primary"> {{/header_left_btn_url}}
                            {{header_left_btn_text}}
                            <i class="{{header_left_btn_icon}}"></i>
                        </a>
                    </div>
                {{/header_left_btn_text}}

                <div class="option-item">
                    {{#header_btn_url}} <a href="{{header_btn_url}}" class="user-btn"> {{/header_btn_url}}
                    {{^ header_btn_url }} <a href="{{ login_url }}" class="user-btn"> {{/header_btn_url}}
                        <i class="{{header_btn_icon}}"></i>
                    </a>
                </div>
            </div>
        {{/isloggedin}}
        
        {{#isloggedin}}
            {{> theme_edoo/edoo_navbar_user }}
        {{/isloggedin}}
    </div>
</div>
<!-- End Navbar Area -->

<!-- Start Mobile Navbar Area -->
<div class="mobile-navbar offcanvas offcanvas-end border-0 navbar-collapse" tabindex="-1" id="navbarOffcanvas">
    <div class="offcanvas-header">
        {{#logo_visibility}}
            <a class="logo d-inline-block" href="{{{ config.wwwroot }}}/">
                {{#mobile_logo}}
                    <img src="{{mobile_logo}}" alt="{{sitename}}"  {{#logo_styles}} style="{{{logo_styles}}}" {{/logo_styles}}>
                {{/mobile_logo}}

                {{^ mobile_logo }}
                    <h2  {{#logo_styles}} style="{{{logo_styles}}}" {{/logo_styles}}>{{sitename}}</h2>
                {{/mobile_logo}}
            </a>
        {{/logo_visibility}}

        <button type="button" class="close-btn bg-transparent position-relative lh-1 p-0 border-0" data-dismiss="offcanvas" aria-label="Close">
            <i class="ri-close-fill"></i>
        </button>
    </div>

    <div class="offcanvas-body">
        <ul class="mobile-menu">
            {{{ output.custom_menu }}}
        </ul>
        {{output.if_edoo_guest_user}}
        {{^ isloggedin}}
            <div class="others-options">
                {{#header_left_btn_text}}
                    <div class="option-item">
                        {{#header_left_btn_url}} <a href="{{header_left_btn_url}}" class="btn btn-primary"> {{/header_left_btn_url}}
                        {{^ header_left_btn_url }} <a href="{{ login_url }}" class="btn btn-primary"> {{/header_left_btn_url}}
                            {{header_left_btn_text}}
                            <i class="{{header_left_btn_icon}}"></i>
                        </a>
                    </div>
                {{/header_left_btn_text}}

                <div class="option-item">
                    {{#header_btn_url}} <a href="{{header_btn_url}}" class="user-btn"> {{/header_btn_url}}
                    {{^ header_btn_url }} <a href="{{ login_url }}" class="user-btn"> {{/header_btn_url}}
                        <i class="{{header_btn_icon}}"></i>
                    </a>
                </div>
            </div>
        {{/isloggedin}}
        
        {{#isloggedin}}
            {{> theme_edoo/edoo_navbar_user }}
        {{/isloggedin}}
    </div>
</div>
<!-- End Mobile Navbar Area -->

